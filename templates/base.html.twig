<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <meta name="robots" content="nofollow">
        <meta name="googlebot" content="noindex">

        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('alerts') }}
        {{ encore_entry_script_tags('locales') }}
        {{ encore_entry_script_tags('blocks') }}
        {{ encore_entry_script_tags('sidebar') }}
        {{ encore_entry_script_tags('pages') }}

        <script
                src="https://code.jquery.com/jquery-3.5.1.min.js"
                integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
                crossorigin="anonymous"></script>

        <link rel="icon"
              type="image/png"
              href="{{ asset("build/images/favicon.png") }}" />

        {% block header %}
        {% endblock %}
    </head>
    <body>
        {% block navigation %}
            {# Todo -> create nav element from bootstrap #}
            {% include "shared/blogfront_menu.html.twig" %}
        {% endblock %}

        {% block flashes %}
            {% include "shared/flashes/success.html.twig" %}
            {% include "shared/flashes/error.html.twig" %}
        {% endblock %}

        {% block body %}{% endblock %}

        {% block javascripts %}


            <script type="text/javascript">
                window.lazyLoadOptions = {};
            </script>

            <script async type="text/javascript"  src="https://cdn.jsdelivr.net/npm/vanilla-lazyload@17.1.0/dist/lazyload.min.js"></script>


        {% endblock %}
    </body>
</html>
